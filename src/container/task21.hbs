<main>
    <div>
        {{#with heading}}
        {{#with main}}
        <div class="card">
            <div class="card-body">
                <div class="card">
                    <div class="card-body">
                        <h1>{{title}}</h1>
                        <p>{{text}}</p>
                        {{#with button_text}}
                        <button class="btn btn-primary">{{solid}}</button>
                    <br>
                    <br>
                        <button class="btn btn-outline-primary">{{outline}}</button>
                    </div>
                    {{/with}}
                    {{/with}}
                </div>
                <br>
                {{#with second}}
                <div class="card">
                    <div class="card-body">
                        <h2>{{title}}</h2>
                        <p>{{text}}</p>
                        {{#with button_text}}
                        <button class="btn btn-secondary">{{solid}}</button>
                    <br>
                    <br>
                        <button class="btn btn-outline-secondary">{{outline}}</button>
                    </div>
                </div>
                {{/with}}
                {{/with}}
                <br>
                {{#with subblock}}
                <div class="card">
                    <div class="card-body">
                        <h3>{{title}}</h3>
                        <p>{{text}}</p>
                        {{#with button_text}}
                        <button class="btn btn-dark">{{solid}}</button>
                    <br>
                    <br>
                        <button class="btn btn-outline-dark">{{outline}}</button>
                    </div>
                </div>
                {{/with}}
                {{/with}}
            {{/with}}
        </div>
    </div>
        <br>
        <br>
        {{#with nested_display}}
        <div class="card">
            <div class="card-body">
                <p class="display-1">{{title}}</p>
                <div class="card">
                    <div class="card-body">
                        {{#with children_display}}
                         <p class="display-4">{{title}}</p>
                         <p>{{text}}</p>
                        <button class="btn btn-outline-warning">{{button.text}}</button>
                    <br>
                    {{/with}}
                    </div>
                </div>
                <br>
                    <h6>{{header_text}}</h6>
                    <button class="btn btn-info">{{button.text}}</button>     
            </div>
        </div>
        {{/with}}
    <br>
    <br>
    {{#with paragraph}}
        <div class="card">
            <div class="card-body">
                <p class="text-uppercase">{{title}}</p>
                <p class="text-lowercase">{{text}}</p>
                    <a href="#" class="btn btn-link">{{button.text}}</a>
            <br>
            <br>
                <div class="card">
                    <div class="card-body">
                        {{#each list_name}}
                        <ul>
                            <li>
                                <a href="#" class="btn btn-link">{{text}}</a>
                            </li>
                        </ul>
                        {{/each}}
                    </div>
                </div>
            </div>
        </div>
        {{/with}}
    </div>
</main>