<div>
<section>
    {{#with navigation}}
        {{#each links}}
            <button class="btn btn-link">
                <a href="{{href}}" class="btn btn-link">{{text}}</a>
            </button>
        {{/each}}
            <button class="btn btn-primary">
                <a href="{{button.href}}" class="btn btn-primary">{{button.text}}</a>
            </button>
    {{/with}}
</section>
    <br>

            <h1>{{header.title}}</h1>
            <br>
            <p>{{header.description}}</p>
             <button class="btn btn-primary">
                <a href="{{header.button.link}}" class="btn btn-primary">{{header.button.text}}</a>
            </button>
        <br>
        <hr>
        <br>

<section>
        <div class="row">
            {{#with city}}
            <h2>{{city.title}}</h2>
            {{#each cards}}
            <div class="col-5">  
                <img class="card-img-top" src="https://picsum.photos/400/200" alt="Product photo">
                {{#with picture}}
                    <a href="{{src}}" class="card-header text-center">{{alt}}</a>
                {{/with}}
                <div class="card-body text-center">
                    <h6 class="card-title"><b>{{title}}</b></h6>
                    <p>{{description}}</p>
                    {{#with button}}
                        <button href={{href}} class="btn btn-primary">{{text}}</button>
                    {{/with}}
                </div>
            </div>
            {{/each}}
            {{/with}}
        </div>
</section>
    <br>
<br> 
    <table class="table table-striped table-bordered">
        {{#with winner}}
        <h2 class="text-center">{{title}}</h2>
    <thead>
        {{#with table}}
        <tr>
            <th>
              #  
            </th>
            <th>
              name  
            </th>
            <th>
              surname  
            </th>
            <th>
              nickname  
            </th>
    </thead>

    <tbody>
        <tr>
            {{#each user}}
            <td>{{place}}</td>
            <td>{{name}}</td>
            <td>{{surname}}</td>
            <td>{{nickname}}</td>
        </tr>
        {{/each}}
        {{/with}}
        {{/with}}
        </tr>
    </tbody>
</table>

<br>
<br>
<br>

<section>
<div class="row">
        {{#with relaxion}}
        <h2 class="text-center">{{title}}</h2>
        {{#each cards}}
                        <div class="col-3">
                            <div class="card text-bg-dark">
                        <div class="card-header text-left">{{header}}</div>
                        <div class="card-top">
                        <ul class="list-group">
                            <li class="list-group-item">{{title}}</li>
                            <li class="list-group-item">{{description}}</li>
                        </ul>
                    </div>
                </div>
            </div>
        {{/each}}
        {{/with}}
        </div>
    </div>
</section>


<br>
<br>

<section>
    <div class="row">
        {{#with rest}}
            <div class="display-3 text-center">Rest</div>
        {{#each cards}}
            <div class="col-6">
                <div class="card">
                    <div class="card">
                        <div class="card-header text-center">{{header}}</div>
                            <div class="card-body text-center">
                                <h6 class="card-title">{{title}}</h6>
                                <p>{{description}}</p>
                                <button class="btn btn-primary">{{button}}</button>
                            </div>
                        <div class="card-footer text-center">{{footer}}</div>
                    </div>
                </div>
            </div>
        {{/each}}
            </div>
        {{/with}}
    </div>
</section>

<br>
<br>
<br>

<section>
    <div class="row">
        {{#with gallery}}
            <div class="display-3 text-center">{{title}}</div>
        {{#each cards}}
            <div class="col-4">
                <div class="card">
                    <div class="card text-bg-warning">
                        <div class="card-header text-center">{{name}}</div>
                        <div class="card-body text-center">
                        <h6 class="card-title">{{items}}</h6>
                        </div>
                    </div>
                </div>
            </div>
        {{/each}}
            </div>
        {{/with}}
    </div>
</section>

<br>
<br>
<br>

<section>
    <div class="row">
        {{#with proposition}}
            <h2 class="text-left">{{title}}</h2>
        {{#each card}}
            <div class="col-4">
                <div class="card">
                <div class="card-header text-center">{{header}}</div>
                    <div class="card-body text-center">
                        <h6 class="card-title">{{title}}</h6>
                        <p>{{description}}</p>
                    </div>
                </div>
            </div>
        {{/each}}
        {{/with}}
    </div>
</section>

<br>
<br>
<br>

<section>
    <div class="row">
            {{#with footer}}
                <h5 class="text-left">{{title}}</h5>
                    <div class="col-3">
                        {{#each navigation}}
                        <li class="list-group-item">
                            <a href="{{href}}" class="btn btn-link">{{text}}</a>
                        </li>
                        {{/each}}
                    </div>
                    <div class="col-3">
                        {{#each socialMedia}}
                        <li class="list-group-item">
                            <a href="{{link}}" class="btn btn-outline-dark">{{name}}</a>
                        </li>
                        {{/each}}
                    </div>
                    <div class="col-3">
                    {{#each partners}}
                        <li class="list-group-item">
                            <a href="{{link}}" class="btn btn-outline-info">{{name}}</a>
                        </li>
                    {{/each}}
                    </div>
            {{/with}}
    </div>
</section>

</div>
