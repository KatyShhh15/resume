<div>
            {{#with navigation}}
                {{#each links}}
                    <button class="btn btn-link">
                        <a href="{{href}}" class="btn btn-link">{{text}}</a>
                    </button>
                {{/each}}
                    <button class="btn btn-primary">
                        <a href="{{button.href}}" class="btn btn-primary">{{button.text}}</a>
                    </button>
            {{/with}}
        <br>

            <h1>{{header.title}}</h1>
            <br>
            <p>{{header.description}}</p>
             <button class="btn btn-primary">
                <a href="{{header.button.link}}" class="btn btn-primary">{{header.button.text}}</a>
            </button>
        <br>
        <hr>
        <br>
        <div>
                <h2>{{city.title}}</h2>
                <br>
                <section class="col-12">
                <div class="row gx-2 gy-2">
                    <div class="col-5">
                        {{#with city}}
                        {{#each cards}}
                        <img class="card-img-top" src="https://picsum.photos/400/200" alt="Product photo">
                        {{#with picture}}
                            <a href="{{src}}" class="card-header text-center">{{alt}}</a>
                        {{/with}}
                                <div class="card-body text-center">
                                    <h6 class="card-title"><b>{{title}}</b></h6>
                                    <p>{{description}}</p>
                                    {{#with button}}
                                        <button href={{href}} class="btn btn-primary">{{text}}</button>
                                {{/with}}
                            {{/each}}
                        {{/with}}
                    </section>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <table class="table table-striped table-bordered">
        {{#with winner}}
        <h2 class="text-center">{{title}}</h2>
    <thead>
        {{#with table}}
        <tr>
            <th>
              #  
            </th>
            <th>
              name  
            </th>
            <th>
              surname  
            </th>
            <th>
              nickname  
            </th>
    </thead>

    <tbody>
        <tr>
            {{#each user}}
            <td>{{place}}</td>
            <td>{{name}}</td>
            <td>{{surname}}</td>
            <td>{{nickname}}</td>
        </tr>
        {{/each}}
        {{/with}}
        {{/with}}
        </tr>
    </tbody>
</table>

<br>
<br>
<br>

<div>
    {{#with gallery}}
    <section>
        <h2 class="text-center">{{title}}</h2>
        <div class="row col-12">
            {{#each cards}}
            <div class="col-4 g-3">
                <div class="card-body">
                    <h3>{{name}}</h3>
                    <p>{{items}}</p>
                </div>
            </div>
            {{/each}}
        </div>
    </section>
    
    {{/with}}
</div>

</div>
